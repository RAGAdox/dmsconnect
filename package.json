{
  "name": "dmsconnect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf .next",
    "ci": "yarn install --frozen-lock",
    "dev": "cross-env NODE_ENV=development next dev --turbopack --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "next build && next start",
    "postinstall": "prisma generate",
    "migrate:up": "prisma migrate deploy && prisma db seed",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset && prisma db seed",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.13.0",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@supabase/supabase-js": "^2.49.4",
    "@upstash/redis": "^1.34.5",
    "classnames": "^2.5.1",
    "formidable": "^3.5.2",
    "lodash": "^4.17.21",
    "next": "^15.2.4",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/formidable": "^3.4.5",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.5",
    "husky": "^9.1.7",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.1.3",
    "typescript": "^5"
  }
}
